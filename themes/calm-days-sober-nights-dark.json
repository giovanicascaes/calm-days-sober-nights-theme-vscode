  "tokenColors": [
    {
      "settings": {
        "background": "#31333B",
        "foreground": "#BEC2D6"
      }
    },
    {
      "name": "Braces",
      "scope": [
        "meta.brace.curly",
        "meta.brace.round",
        "meta.brace.square",
        "meta.structure.dictionary",
        "punctuation.section",
        "punctuation.definition.block",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#BEC2D69A"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#71768C"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#F2B7FE"
      }
    },
    {
      "name": "String Regexp",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#F2B7FE"
      }
    },
    {
