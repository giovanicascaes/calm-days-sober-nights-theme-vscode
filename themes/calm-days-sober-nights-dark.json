  "tokenColors": [
    {
      "settings": {
        "background": "#35283B",
        "foreground": "#CDBCD6"
      }
    },
    {
      "name": "Braces",
      "scope": [
        "meta.brace.curly",
        "meta.brace.round",
        "meta.brace.square",
        "meta.structure.dictionary",
        "punctuation.section",
        "punctuation.definition.block",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#CDBCD69A"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#826D8C"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#EBB7FE"
      }
    },
    {
      "name": "String Regexp",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#EBB7FE"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "constant.character",
        "constant.other",
        "punctuation.definition.group.regexp"
