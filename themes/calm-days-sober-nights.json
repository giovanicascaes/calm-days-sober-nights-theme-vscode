    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.terminator", "meta.delimiter.comma"],
      "settings": {
        "foreground": "#969994"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5c6773"
      }
