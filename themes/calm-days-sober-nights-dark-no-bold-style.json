    },
    {
      "name": "Imports and packages",
      "scope": ["entity.name.import", "entity.name.package"],
      "settings": { "foreground": "#FF9EA8" }
    },
    {
      "name": "Entity name",
      "scope": ["entity.name"],
      "settings": { "foreground": "#3CEBE2" }
    },
    {
      "name": "Static types",
