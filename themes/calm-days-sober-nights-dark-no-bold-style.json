  "tokenColors": [
    { "settings": { "background": "#2D2A3B", "foreground": "#B3ADCC" } },
    {
      "name": "Braces",
      "scope": [
        "meta.brace.curly",
        "meta.brace.round",
        "meta.brace.square",
        "meta.structure.dictionary",
        "punctuation.section",
        "punctuation.definition.block",
        "punctuation.definition.parameters"
      ],
      "settings": { "foreground": "#B3ADCCB0" }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": { "fontStyle": "italic", "foreground": "#5A5573" }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#FF9EA8" }
    },
    {
      "name": "String Regexp",
      "scope": ["string.regexp"],
      "settings": { "foreground": "#FF9EA8" }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "constant.character",
        "constant.other",
        "punctuation.definition.group.regexp"
      ],
      "settings": { "foreground": "#7AF5CE" }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape.backslash.regexp"],
      "settings": { "foreground": "#FF8785" }
    },
    {
      "name": "Anchor Escape Characters",
      "scope": ["keyword.control.anchor.regexp"],
      "settings": {}
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#FF8785" }
    },
    {
      "name": "Built-in constants",
      "scope": ["constant.language"],
      "settings": { "foreground": "#FF8785" }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": { "foreground": "#B3ADCC" }
    },
    {
      "name": "Member Variable",
      "scope": ["variable.member"],
      "settings": { "foreground": "#FA78A5" }
    },
    {
      "name": "Language variable",
      "scope": ["variable.language"],
      "settings": { "fontStyle": "italic", "foreground": "#4FD6BE" }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": { "foreground": "#2EBFFF" }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": { "foreground": "#2EBFFF" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#FA78A5" }
    },
    {
      "name": "Separators like ; or ,",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "meta.delimiter.comma"
      ],
      "settings": { "foreground": "#B3ADCC96" }
    },
    {
      "name": "Accessor",
      "scope": ["punctuation.accessor"],
      "settings": { "foreground": "#FA78A5" }
    },
    {
      "name": "Types fixes",
      "scope": [
        "source.java storage.type",
        "source.haskell storage.type",
        "source.c storage.type",
        "meta.type.parameters"
      ],
      "settings": { "foreground": "#FA78A5" }
    },
    {
      "name": "Inherited class type",
      "scope": ["entity.other.inherited-class"],
      "settings": { "foreground": "#4FD6BE" }
    },
    {
      "name": "Lambda arrow",
      "scope": ["storage.type.function"],
      "settings": { "foreground": "#FA78A5" }
    },
    {
      "name": "Java primitive variable types",
      "scope": ["source.java storage.type.primitive"],
      "settings": { "foreground": "#4FD6BE" }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": { "foreground": "#37D1E6" }
    },
    {
      "name": "Function arguments",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": { "foreground": "#B294FF" }
    },
    {
      "name": "Function call",
      "scope": [
        "variable.function",
        "variable.annotation",
        "meta.function-call.generic",
