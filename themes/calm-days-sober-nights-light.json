  "tokenColors": [
    {
      "settings": {
        "background": "#F7F7F7",
        "foreground": "#5E6B7A"
      }
    },
    {
      "name": "Braces",
      "scope": [
        "meta.brace.curly",
        "meta.brace.round",
        "meta.brace.square",
        "meta.block",
        "punctuation.section",
        "punctuation.definition.block",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#5E6B7AA0"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A5B4C2"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#E66220"
      }
    },
    {
      "name": "String Regexp",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#E66220"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "constant.character",
        "constant.other",
        "punctuation.definition.group.regexp"
      ],
      "settings": {
        "foreground": "#57AD83"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape.backslash.regexp"],
      "settings": {
        "foreground": "#6444A3"
      }
    },
    {
      "name": "Anchor Escape Characters",
      "scope": ["keyword.control.anchor.regexp"],
      "settings": {}
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#6444A3"
      }
    },
    {
      "name": "Built-in constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#6444A3"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#5E6B7A"
      }
    },
    {
      "name": "Member Variable",
      "scope": ["variable.member"],
      "settings": {
        "foreground": "#C75399"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#1FA9DB"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#1FA9DB"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#C75399"
      }
    },
    {
      "name": "Separators like ; or ,",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "meta.delimiter.comma"
      ],
      "settings": {
        "foreground": "#5E6B7A76"
      }
    },
    {
      "name": "Accessor",
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#C75399"
      }
    },
    {
      "name": "Types fixes",
      "scope": [
        "source.java storage.type",
        "source.haskell storage.type",
        "source.c storage.type",
        "meta.type.parameters"
      ],
      "settings": {
        "foreground": "#C75399"
      }
    },
    {
      "name": "Inherited class type",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#7E9E26"
      }
    },
    {
      "name": "Lambda arrow",
      "scope": ["storage.type.function"],
      "settings": {
        "foreground": "#C75399"
      }
    },
    {
      "name": "Java primitive variable types",
      "scope": ["source.java storage.type.primitive"],
      "settings": {
        "foreground": "#7E9E26"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#71B6D2"
      }
    },
    {
      "name": "Function arguments",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#9F93B8"
      }
    },
    {
      "name": "Function call",
      "scope": [
        "variable.function",
        "variable.annotation",
        "meta.function-call.generic",
        "support.function.go"
      ],
      "settings": {
        "foreground": "#71B6D2"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function", "support.macro"],
      "settings": {
        "foreground": "#C767A1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Imports and packages",
