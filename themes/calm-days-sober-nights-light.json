      "settings": {
        "foreground": "#7E9E2680"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#71B6D2"
      }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C75399"
      }
    },
    {
      "name": "Library class/type",
